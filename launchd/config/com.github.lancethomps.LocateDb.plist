<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>com.github.lancethomps.${LAUNCHD_SCRIPT_ID}.plist</string>
    <key>ProgramArguments</key>
    <array>
      <string>/usr/local/bin/bash</string>
      <string>-c</string>
      <string>"${LAUNCHD_SCRIPT_PATH}"</string>
    </array>
    <key>LowPriorityIO</key>
    <true/>
    <key>Nice</key>
    <integer>5</integer>
    <key>KeepAlive</key>
    <dict>
      <key>PathState</key>
      <dict>
        <key>/Users/${USER}/locate.database</key>
        <false/>
      </dict>
    </dict>
    <key>RunAtLoad</key>
    <true/>
    <key>StartInterval</key>
    <integer>43200</integer>
    <key>StandardOutPath</key>
    <string>/Users/${USER}/Library/Logs/lt_logs/com.github.lancethomps.launchd.log</string>
    <key>StandardErrorPath</key>
    <string>/Users/${USER}/Library/Logs/lt_logs/com.github.lancethomps.launchd.log</string>
    <key>AbandonProcessGroup</key>
    <true/>
  </dict>
</plist>
